; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="COMMONGROUNDS"
config/version="infdev1.0"
run/main_scene="res://scenes/Game.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/use_filter=false
config/icon="res://icon.svg"

[autoload]

DebugMenu="*res://addons/debug_menu/debug_menu.tscn"
TextTransitionSettings="*res://addons/awesome_label/text_transition_settings.gd"
EventManager="*res://src/EventManager.gd"
MouseManager="*res://src/MouseManager.tscn"
Chat="*res://src/Chat.tscn"
NG="*res://src/NG.gd"
Log="*res://src/Log.gd"
System="*res://src/System.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/aspect="ignore"
window/handheld/orientation=6
window/vsync/vsync_mode=0

[editor]

movie_writer/fps=120

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/awesome_label/plugin.cfg", "res://addons/camera_2d_plus/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/smoothing/plugin.cfg")

[file_customization]

folder_colors={
"res://resources/": "orange",
"res://scenes/": "teal",
"res://src/": "blue"
}

[gui]

theme/custom_font="res://resources/fonts/AlteHaasGroteskBold.ttf"
theme/default_font_multichannel_signed_distance_field=true

